{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://eq.ons.gov.uk/rules/operators/array/all-in.json",
  "description": "Evaluates if all values from the first array are in the second array",
  "type": "object",
  "properties": {
    "all-in": {
      "type": "array",
      "minItems": 2,
      "maxItems": 2,
      "items": {
        "$ref": "https://eq.ons.gov.uk/rules/definitions.json#/array_value"
      }
    }
  },
  "required": ["all-in"],
  "additionalProperties": false
}
