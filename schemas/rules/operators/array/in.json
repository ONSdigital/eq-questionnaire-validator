{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://eq.ons.gov.uk/rules/operators/array/in.json",
  "description": "Evaluates if a value is in an array",
  "type": "object",
  "properties": {
    "in": {
      "type": "array",
      "minItems": 2,
      "maxItems": 2,
      "items": [
        {
          "$ref": "https://eq.ons.gov.uk/rules/definitions.json#/any_value_except_array"
        },
        {
          "$ref": "https://eq.ons.gov.uk/rules/definitions.json#/array_value"
        }
      ]
    }
  },
  "required": ["in"],
  "additionalProperties": false
}
