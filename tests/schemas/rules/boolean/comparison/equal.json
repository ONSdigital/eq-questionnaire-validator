[
  {
    "description": "Two non-array values",
    "rule": {
      "==": [1, 1]
    }
  },
  {
    "description": "Two array values",
    "rule": {
      "==": [[], []]
    }
  },
  {
    "description": "An answer and a value",
    "rule": {
      "==": [
        {
          "source": "answers",
          "identifier": "test"
        },
        "Yes"
      ]
    }
  },
  {
    "description": "An un-answered answer",
    "rule": {
      "==": [
        {
          "source": "answers",
          "identifier": "test"
        },
        null
      ]
    }
  },
  {
    "description": "Compare an answer to a boolean",
    "rule": {
      "==": [
        {
          "source": "answers",
          "identifier": "test"
        },
        true
      ]
    }
  }
]
